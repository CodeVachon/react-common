    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
